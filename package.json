{
  "name": "full-stack-example",
  "author": "Lance Hsu <lancehsu12@gmail.com>",
  "version": "1.0.0",
  "description": "An example of full-stack development using React-Redux & Node Express & MongoDB",
  "main": "index.js",
  "engines": {
    "node": "14.9.0"
  },
  "license": "MIT",
  "scripts": {
    "setup": "npm install && cd ../client && npm install && cd ..",
    "dev": "concurrently \"npm start\" \"cd client && npm start\"",
    "build": "cd ./client && npm install && npm run build && cp -r build/* ../build/",
    "start": "node ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lancehsu/full-stack-example.git"
  },
  "keywords": [
    "react",
    "redux",
    "node",
    "express",
    "MongoDB"
  ],
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.16.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express-session": "^1.15.6",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.2",
    "install": "^0.12.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.7.7",
    "mongoose-currency": "^0.2.0",
    "morgan": "^1.9.1",
    "multer": "^1.3.1",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "session-file-store": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
